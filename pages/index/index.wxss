/**index.wxss**/
.search-box{
	 /* background-color: lightblue;  */
	position: relative;
	height: 80rpx;
	width: 100%;
	padding: 10rpx 60rpx;
	padding-top: 100rpx;
}
.search-text{
	position: absolute;	
	font-size: larger;
	color: #205064;
	width: 100rpx;
	font-weight: 500;
}

.search{
	position: absolute;
	left: 180rpx;
	background-color: #d7e8f8;
	border-radius: 50rpx;
	height: 60rpx;
	width: 500rpx;
}

.question{
	position: absolute;
	width: 35rpx;
	height: 35rpx;
	margin: 12rpx;
	margin-left: 30rpx;
}

.input{
	margin-left: 100rpx;
	padding:8rpx;
	font-weight: lighter;
	font-size: small;
}

.mid-box{
 /* background-color: yellow; */
	position: relative;
	width: 100%;
	height: 450rpx;
	display: flex; /* 添加 Flexbox 布局 */
  justify-content: center; /* 在主轴方向居中对齐 */
}

.mid-sort{
	
	width: 86%;
	height: 100rpx;
	display: flex; /* 添加 Flexbox 布局 */
  justify-content: center; /* 在主轴方向居中对齐 */
}

.sort-name{
	
	float: left;
	justify-content: space-between;
	width: 150rpx;
	display: flex; /* 添加 Flexbox 布局 */
	justify-content: center; /* 在主轴方向居中对齐 */
	padding-top: 10rpx;
	color: #205064;
	font-size: 28rpx;
} 

.sort-name-checked{
	float: left;
	justify-content: space-between;
	width: 150rpx;
	display: flex; /* 添加 Flexbox 布局 */
	justify-content: center; /* 在主轴方向居中对齐 */
	padding-top: 10rpx;
	color: #205064;
	font-size: 28rpx;
	font-weight: bolder;
	text-decoration:solid underline 3px;
}





.mid-item-list{
	position: absolute;
	height: 375rpx;
	width: 100%;
	bottom: 0rpx;
	
}

.mid-item{
	position: relative;
	background-color: white;
	margin: 50rpx 20rpx;
	margin-top: 10rpx;
	height: 330rpx;
	width: 250rpx;
	border-radius: 25rpx;
	box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.2);
	padding: 10rpx;
	display: flex; /* 添加 Flexbox 布局 */
	justify-content: center; /* 在主轴方向居中对齐 */
}

.item-img{
	position: absolute;
	width: 250rpx;
	height: 250rpx;
	border-radius: 25rpx;
	object-fit:cover;
}

.item-name{
	position: absolute;
	bottom: 20rpx;
	font-weight:lighter;
	font-size: 25rpx;
}


.new-box{
	/* background-color: green;  */
	width: 100%;
	height: 200rpx;
	position: relative;
	padding: 20rpx;
}

.new-title{
	font-weight: bold;
	color: #205064;
	font-size: 30rpx;
	margin-bottom: 30rpx;
}

.news{
	background-color:rgba(128, 128, 128, 0.226) ;
	width: 80%;
	height: 60rpx;
	margin-left: 20rpx;
	padding: 20rpx 40rpx;
	font-size:20rpx;
	border-radius: 50rpx;
	line-height: 1.5;
}

.news-swiper{
	overflow: hidden;
	width: 600rpx;
	height: 60rpx;
}

