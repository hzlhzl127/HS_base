.foot{
	
	padding: 20rpx;
	width: 100%;
}

.foot-list{
	
	width: 94%;
}

.foot-item{
	width: 100%;
	height: 300rpx;
	position: relative;
	margin-bottom: 40rpx;
}

.foot-img{
	width: 100%;
	height: 100%;
	border-radius: 25rpx;
}

.img-name{
	position: absolute;
	bottom: 100rpx;
	left: 40rpx;
	color: white;
	font-size: 25rpx;
	font-weight: bold;
}

.img-intro{
	position: absolute;
	bottom: 35rpx;
	left: 60rpx;
	color: white;
	font-size: 20rpx;
	width: 600rpx;
	height: 50rpx;
	overflow: hidden;
}

.mid-item-list swiper {
  width: 100%; /* 设置swiper的宽度，根据实际需求调整 */
  height: 300px; /* 设置swiper的高度，根据实际需求调整 */
  overflow: hidden;
  display: flex; /* 使用flexbox布局 */
	 /* 将swiper-item平分宽度空间 */
	justify-content: space-between; /* 将swiper-item平分宽度空间 */
}

.mid-item-list swiper-item {
  width: 20%; /* 设置每个swiper-item的宽度为屏幕宽度的20%，这里有5个swiper-item，所以每个平分宽度 */
	flex-shrink: 0; /* 防止swiper-item缩小 */
}

.tabarBox{
	height: 100rpx;
	width: 100%;
	position: fixed;
	bottom: 0rpx;
	display: flex;
  justify-content: center;
}

.tabar {
  display: flex;
  justify-content: center; /* 水平居中对齐 */
  height: 100rpx;
  width: 500rpx;
  background-color: #dfe4e79f;
  border-radius: 100rpx 100rpx 0 0;
}

.tabar-icon{
	height: 60rpx;
	width: 60rpx;
	margin: 20rpx 80rpx;
}

